            <script type="text/javascript">//<![CDATA[
    function initChildCategory(){
        var children = $('div.child_category');
        var parents = $('div.parent_category');
        
        parents.find('select').on('change',function(){
            var parent_id = $(this).val();
            if(parent_id > 0){
                $.each(children.find('div[class^=child_category_]'),function(){
                    if($(this).hasClass('child_category_'+parent_id)){
                        $(this).find('select').removeAttr('disabled');
                        $(this).show();
                        children.show();
                    }else{
                        $(this).find('select').val('');
                        $(this).find('select').attr('disabled','disabled');
                        $(this).hide();
                    }
                });
            }else{
                $.each(children.find('div[class^=child_category_]'),function(){
                    $(this).find('select').val('');
                    $(this).find('select').attr('disabled','disabled');
                    $(this).find('select').hide();
                    children.hide();
                });
            }

        });
        
        var selected = parents.find('select').val();
        if(selected>0){
            children.find('select').attr('disabled','disabled');
            $('.child_category_'+selected).find('select').removeAttr('disabled');
            $('.child_category_'+selected).show();
            children.show();
        }else{
            
        }
    }
    $(function(){
        initChildCategory();
    });
//]]></script>


                            <div class="clm clm_c picker">
                                <input id="picker_start" name="sale_date_before" class="rich" type="text" value="" maxlength="10" />
                                <a href="" title="" onclick="$(this).siblings('input:first').click();
                                        return false;"><img src="/user_data/packages/responsive/img/icons/calendar.gif" width="15" height="16" alt="カレンダー" /></a><br>
                                &nbsp;〜&nbsp;<br>
                                <input class="rich" name="sale_date_after" id="picker_end" type="text" value="" maxlength="10" />
                                <a href="" title="" onclick="$(this).siblings('input:last').click();
                                        return false;"><img src="/user_data/packages/responsive/img/icons/calendar.gif" width="15" height="16" alt="カレンダー" /></a>
                                <script type="text/javascript">
                                    start = $('#picker_start').val();
                                    end = $('#picker_end').val();
                                    if (!start) {
                                        today = new Date();
                                        y = today.getFullYear();
                                        mm = ("0" + (today.getMonth() + 1)).slice(-2);
                                        dd = ("0" + today.getDate()).slice(-2);
                                        start = y + '/' + mm + '/' + dd;
                                    }
                                    if (!end) {
                                        today = new Date();
                                        y = today.getFullYear();
                                        mm = ("0" + (today.getMonth() + 1)).slice(-2);
                                        dd = ("0" + today.getDate()).slice(-2);
                                        end = y + '/' + mm + '/' + dd;
                                    }

                                    $('#picker_start').DatePicker({
                                        format: 'Y/m/d',
                                        date: start,
                                        current: start,
                                        starts: 0,
                                        position: 'r',
                                        onBeforeShow: function() {
                                            $('#picker_start').DatePickerSetDate(start, true);
                                        },
                                        onChange: function(formated, dates) {
                                            $('#picker_start').val(formated).DatePickerHide();
                                        }
                                    });
                                    $('#picker_end').DatePicker({
                                        format: 'Y/m/d',
                                        date: end,
                                        current: end,
                                        starts: 0,
                                        position: 'r',
                                        onBeforeShow: function() {
                                            $('#picker_end').DatePickerSetDate(end, true);
                                        },
                                        onChange: function(formated, dates) {
                                            $('#picker_end').val(formated).DatePickerHide();
                                        }
                                    });
                                </script>
                            </div>
                        </div>

         <script>
    var brand = "tora";
    function clickHandlerTwitter() {
      gtag('event', '外部リンク_Twitter', {'event_category': 'ボタンクリック', 'event_label': 'ドロワー（' + brand + '）'});
    }
    $(function(){
      var url = 'https://twitter.com/tora_ec_male_u/';
      switch(brand) {
        case 'joshi':
        case 'joshi_r':
          url = 'https://twitter.com/tora_ec_jyosei'
          break;
        case 'tora_r':
          url = 'https://twitter.com/tora_ec_male_18'
          break;
      }
      // DOM構築後にリンクを設定
      $('.js-drawer-twitter-link').attr({href: url});
    })
  </script> 
         
             
             
